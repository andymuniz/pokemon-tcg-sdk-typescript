  "devDependencies": {
    "@types/node": "^17.0.35",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-dts": "^1.2.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/pokemon-tcg-client.umd.js",
  "module": "./dist/pokemon-tcg-client.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
