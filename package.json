{
  "name": "@andymuniz/pokemon-tcg-sdk-typescript",
  "version": "0.1.6",
  "description": "Unofficial TypeScript SDK for the Pokemon TCG API. (https://pokemontcg.io)",
  "author": "Andy Muniz",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "exit 0"
